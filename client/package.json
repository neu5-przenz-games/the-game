{
    "name": "client",
    "private": true,
    "version": "0.0.1",
    "repository": "https://github.com/neu5-przenz-games/the-game",
    "author": "neu5",
    "license": "MIT",
    "alias": {
        "shared": "./shared"
    },
    "_moduleAliases": {
        "shared": "./shared"
    },
    "dependencies": {
        "@geckos.io/snapshot-interpolation": "^1.0.2",
        "phaser": "^3.52.0"
    },
    "devDependencies": {
        "parcel": "^2.1.1",
        "parcel-reporter-clean-dist": "^1.0.3",
        "parcel-reporter-static-files-copy": "^1.3.4",
        "sass": "^1.35.1",
        "socket.io-client": "^4.1.2"
    },
    "staticFiles": {
        "staticPath": "../public"
    },
    "packageManager": "yarn@3.1.1",
    "scripts": {
        "prebuild": "yarn generateMocks && yarn extract:map",
        "build": "yarn prebuild && bash scripts/bash/build.sh && yarn postbuild",
        "build:prod": "NODE_ENV=production parcel build client/index.html",
        "build:dev": "NODE_ENV=development parcel build client/index.html --no-optimize",
        "build:test": "MAP=test yarn generateMocks && MAP=test yarn extract:map && NODE_ENV=development parcel build client/index.html --no-optimize",
        "build:html": "node ./scripts/generateStaticPages.mjs",
        "postbuild": "yarn build:html",
        "predev": "yarn generateMocks && yarn extract:map",
        "dev": "parcel watch index.html",
        "dev:mini": "MAP=mini yarn dev",
        "dev:test": "MAP=test yarn dev"
    }
}
